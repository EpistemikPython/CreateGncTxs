{
    "version": 1 ,
    "formatters": {
        "simple": {
            "format": "%(filename)s - %(funcName)s - %(lineno)s - %(name)s - %(levelname)s - %(processName)s - %(message)s"
        } ,
        "complex": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        }
    } ,
    "handlers": {
        "console": {
            "class": "logging.StreamHandler" ,
            "level": "DEBUG" ,
            "formatter": "simple" ,
            "stream": "ext://sys.stdout"
        } ,
        "fileh": {
            "class": "logging.FileHandler" ,
            "level": "INFO" ,
            "formatter": "complex" ,
            "filename": "gnucash.log"
        }
    } ,
    "loggers": {
        "gnucash": {
            "level": "INFO" ,
            "handlers": ["console","fileh"] ,
            "propagate": "no"
        } ,
        "root": {
            "level": "DEBUG" ,
            "handlers": ["console"]
        }
    }
}
